<h1>User Login</h1>

<%= render partial: 'shared/error_messages', locals: { object: @user } %>

<%= form_for :login do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>

  <%= f.submit "Login" %>

<% end %>